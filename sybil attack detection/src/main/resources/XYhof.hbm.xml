<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="Initial.XYhof" table="xyhof" catalog="m0">
        
        <id name="Id" type="int">
            <column name="id" />
            <generator class="identity"></generator>
        </id>
        <property name="node" type="int">
            <column name="node" length="11" not-null="true" unique="true" />
        </property>
        <property name="xvalue" type="int">
            <column name="xvalue" length="11" not-null="true" unique="true" />
        </property>
        <property name="yvalue" type="int">
            <column name="yvalue" length="11" not-null="true" unique="true" />
        </property>
        <property name="type" type="string">
            <column name="type" length="45" not-null="true" unique="true" />
        </property>
        
        
    </class>
</hibernate-mapping>